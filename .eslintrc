{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
  ],
  "rules": {
    "react/require-extension": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "settings": {
    "import/core-modules": [ "meteor/meteor" ]
  }
}
